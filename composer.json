    "authors": [
        {
            "name": "NetCommons Community",
            "homepage": "https://github.com/NetCommons3/Notifications/graphs/contributors"
        }
    ],
    "keywords": ["cakephp", "notifications"],
    "config": {
        "vendor-dir": "vendors"
    }
}

